{
  "installCommand": "pnpm install --frozen-lockfile",
  "buildCommand": "pnpm run build:client",
  "outputDirectory": "dist/spa",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
